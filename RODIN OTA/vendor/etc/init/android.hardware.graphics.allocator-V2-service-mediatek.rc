service vendor.gralloc-v2 /vendor/bin/hw/android.hardware.graphics.allocator-V2-service-mediatek
    class hal animation
    user system
    group graphics drmrpc
    capabilities SYS_NICE
    onrestart restart surfaceflinger
    onrestart restart vendor.hwcomposer
    onrestart restart camerahalserver
    writepid /sys/kernel/mi_mempool/pids /proc/sys/binder_prio/gallocator_pid /sys/kernel/reserve_pool/pid /sys/module/metis/parameters/set_static_vip
